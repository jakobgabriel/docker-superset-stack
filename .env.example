# --- App version pinning ---
SUPERSET_TAG=latest

# --- Admin bootstrap (used only by the init job) ---
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change_me_now
ADMIN_FIRST_NAME=Admin
ADMIN_LAST_NAME=User
ADMIN_EMAIL=admin@superset.local

# --- Security ---
SUPERSET_SECRET_KEY=replace_with_32+_random_chars

# --- Metadata DB (Postgres) ---
POSTGRES_DB=superset
POSTGRES_USER=superset
POSTGRES_PASSWORD=strong_pg_password

# --- Optional: Geospatial maps ---
MAPBOX_API_KEY=

# --- Gunicorn tuning (optional) ---
GUNICORN_WORKERS=4
GUNICORN_TIMEOUT=120

# --- Celery tuning (optional) ---
CELERY_CONCURRENCY=4
CELERY_REPORTS_CONCURRENCY=2
