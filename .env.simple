# Minimal Superset configuration for docker-compose.simple.yml

# SECURITY: change this to a strong, random string
SUPERSET_SECRET_KEY=please_change_me

# Admin user bootstrap (created on first start)
ADMIN_USERNAME=admin
ADMIN_FIRST_NAME=Admin
ADMIN_LAST_NAME=User
ADMIN_EMAIL=admin@superset.local
ADMIN_PASSWORD=admin

# Optional: load example dashboards (yes/no)
SUPERSET_LOAD_EXAMPLES=no

# Optional: Mapbox token for map visuals
# MAPBOX_API_KEY=

